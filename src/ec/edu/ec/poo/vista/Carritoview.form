<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="ec.edu.ec.poo.vista.CarritoView">
  <grid id="carritoGrid" binding="panelPrincipal" layout-manager="GridLayoutManager" row-count="4" column-count="3" same-size-horizontally="false" same-size-vertically="false" hgap="-1" vgap="-1">
    <margin top="30" left="30" bottom="30" right="30"/>
    <constraints>
      <xy x="20" y="20" width="650" height="400"/>
    </constraints>
    <properties>
      <background color="-6891645"/>
    </properties>
    <border type="line" title="CARRITO DE COMPRAS" title-justification="2">
      <font size="14" style="1"/>
      <color color="-16777216"/>
    </border>
    <children>
      <component id="titulo" class="javax.swing.JLabel">
        <constraints>
          <grid row="0" column="0" row-span="1" col-span="3" vsize-policy="0" hsize-policy="0" anchor="4" fill="0" indent="0" use-parent-layout="false"/>
        </constraints>
        <properties>
          <font name="Courier New" size="18" style="1"/>
          <horizontalAlignment value="0"/>
          <text value="Lista de Productos en el Carrito"/>
        </properties>
      </component>
      <component id="labelTotal" class="javax.swing.JLabel" binding="labelTotal">
        <constraints>
          <grid row="2" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="0" anchor="4" fill="0" indent="0" use-parent-layout="false"/>
        </constraints>
        <properties>
          <font name="Courier New" size="14" style="1"/>
          <text value="Total: $0.00"/>
        </properties>
      </component>
      <component id="txtDescuento" class="javax.swing.JTextField" binding="txtDescuento">
        <constraints>
          <grid row="2" column="1" row-span="1" col-span="1" vsize-policy="0" hsize-policy="6" anchor="0" fill="1" indent="0" use-parent-layout="false"/>
        </constraints>
        <properties>
          <toolTipText value="Descuento (%)"/>
        </properties>
      </component>
      <component id="btnAplicarDescuento" class="javax.swing.JButton" binding="btnAplicarDescuento">
        <constraints>
          <grid row="2" column="2" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="0" fill="1" indent="0" use-parent-layout="false"/>
        </constraints>
        <properties>
          <font name="Courier New" size="14"/>
          <text value="Aplicar Descuento"/>
        </properties>
      </component>
      <component id="btnAgregar" class="javax.swing.JButton" binding="btnAgregar">
        <constraints>
          <grid row="3" column="0" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="0" fill="1" indent="0" use-parent-layout="false"/>
        </constraints>
        <properties>
          <font name="Courier New" size="14"/>
          <text value="Agregar Producto"/>
        </properties>
      </component>
      <component id="btnEliminar" class="javax.swing.JButton" binding="btnEliminar">
        <constraints>
          <grid row="3" column="1" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="0" fill="1" indent="0" use-parent-layout="false"/>
        </constraints>
        <properties>
          <font name="Courier New" size="14"/>
          <text value="Eliminar Producto"/>
        </properties>
      </component>
      <component id="btnPagar" class="javax.swing.JButton" binding="btnPagar">
        <constraints>
          <grid row="3" column="2" row-span="1" col-span="1" vsize-policy="0" hsize-policy="3" anchor="0" fill="1" indent="0" use-parent-layout="false"/>
        </constraints>
        <properties>
          <font name="Courier New" size="14"/>
          <text value="Pagar"/>
        </properties>
      </component>
      <component id="tableCarrito" class="javax.swing.JTable" binding="tableCarrito">
        <constraints>
          <grid row="1" column="1" row-span="1" col-span="1" vsize-policy="0" hsize-policy="0" anchor="0" fill="1" indent="0" use-parent-layout="false"/>
        </constraints>
        <properties/>
      </component>
    </children>
  </grid>
</form>
